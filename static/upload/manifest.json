{
  "name": "MrMatt.io Photo Upload",
  "short_name": "Photo Upload",
  "start_url": "/upload/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#090909",
  "icons": [
    {
      "src": "/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/android-chrome-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "share_target": {
    "action": "/upload/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "caption",
      "files": [
        {
          "name": "photo",
          "accept": ["image/jpeg", "image/png", "image/webp"]
        }
      ]
    }
  }
}
